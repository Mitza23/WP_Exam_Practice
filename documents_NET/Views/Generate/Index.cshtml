@{
    Layout = "../Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Your document</title>
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script>
        $(document).ready(function () {
            generateDocument();
        });

        function generateDocument() {
            var documentId = sessionStorage["documentId"];

            $.get("/Generate/GetDocument", { documentId: documentId },
                function (data, status) {
                    console.log(data);
                    $("#maindiv").html(data);
                });
        }

    </script>
</head>
<body>
    <h3>Your document</h3>

    <div id="maindiv"></div>
</body>
</html>